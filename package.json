{
  "name": "suiknit",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "typecheck": "vue-tsc --noEmit -p tsconfig.app.json",
    "typecheck:watch": "vue-tsc --noEmit -p tsconfig.app.json -w --preserveWatchOutput",
    "typecheck:strict": "yarn typecheck && eslint \"src/**/*.{ts,tsx}\" --max-warnings=0",
    "build": "yarn typecheck && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,vue,tsx,js,jsx}": [
      "eslint --fix"
    ],
    "*.{css,scss,md,html,json,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@alova/adapter-axios": "^2.0.16",
    "@alova/adapter-xhr": "^2.2.1",
    "@formkit/icons": "^1.6.9",
    "@formkit/themes": "^1.6.9",
    "@formkit/vue": "^1.6.9",
    "@vueuse/core": "^13.9.0",
    "alova": "^3.3.4",
    "axios": "^1.13.2",
    "chart.js": "^4.5.0",
    "dayjs": "^1.11.18",
    "echarts": "^6.0.0",
    "gsap": "^3.13.0",
    "pinia": "^3.0.3",
    "rrweb": "^2.0.0-alpha.4",
    "vue": "^3.5.21",
    "vue-chartjs": "^5.3.2",
    "vue-echarts": "^8.0.0",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.36.0",
    "@iconify-json/heroicons": "^1.2.3",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^24.5.2",
    "@types/nprogress": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitalets/google-translate-api": "^9.2.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^4.0.8",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.0.0",
    "daisyui": "^5.1.25",
    "eslint": "^9.36.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.5.0",
    "happy-dom": "^20.0.10",
    "husky": "^9.1.7",
    "json-format": "^1.0.1",
    "lint-staged": "^16.2.3",
    "mockjs": "^1.1.0",
    "msw": "^2.11.3",
    "nprogress": "^0.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^4.1.13",
    "tunnel": "^0.0.6",
    "typescript": "~5.8.3",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-icons": "^22.3.0",
    "unplugin-vue-components": "^29.1.0",
    "vite": "^7.1.7",
    "vite-auto-i18n-plugin": "^1.1.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-inspect": "^11.3.3",
    "vite-plugin-ops": "^0.1.1",
    "vite-plugin-pages": "^0.33.1",
    "vite-plugin-pwa": "^1.0.3",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vitest": "^4.0.8",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.7"
  }
}
